{
    "extends": "@workspace/typescript-config/base.json",
    "compilerOptions": {
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true
    },
    "include": ["embed.ts","vite.config.ts","vite-env.d.ts","**/*.ts","**/*.tsx"],
    "exclude": ["node_modules", "dist", "build", "**/*.spec.ts", "**/*.spec.tsx"]
}